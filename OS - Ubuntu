<p>&nbsp;</p>

<p>&nbsp;</p>

<pre>
#! /bin/bash
f_sqlite3=&quot;http://www.sqlite.org/2013/sqlite-autoconf-3071600.tar.gz&quot;
f_proj=&quot;http://download.osgeo.org/proj/proj-4.8.0.tar.gz&quot;
f_geos=&quot;http://download.osgeo.org/geos/geos-3.3.8.tar.bz2&quot;
f_freexl=&quot;http://www.gaia-gis.it/gaia-sins/freexl-1.0.0e.tar.gz&quot;
f_spatialite=&quot;http://www.gaia-gis.it/gaia-sins/libspatialite-sources/libspatialite-4.0.0.tar.gz&quot;
p_configure=&quot;--build=x86_64-pc-linux-gnu --host=arm-linux-gnueabi CC=arm-linux-gnueabi-gcc LDFLAGS=-L/usr/local/lib CPPFLAGS=-I/usr/local/include&quot;

sudo apt-get install -y build-essential make gcc g++ gcc-arm-linux-gnueabi
sudo apt-get install -y git


if [ ! -d &quot;spatialite-android&quot; ]; then
&nbsp; git clone https://code.google.com/p/spatialite-android/
else
&nbsp; echo &quot;spatialite-android/ is existing&quot;
fi
cd spatialite-android/spatialite-android-library/jni/



s=`ls sqlite-autoconf-*.tar.*`
echo $s
if [ -z &quot;$s&quot; ]; then
&nbsp; wget $f_sqlite3
&nbsp; s=`ls sqlite-autoconf-*.tar.*`
else
&nbsp; echo &quot;$s is ok&quot;
fi
d=${s%.tar*}
if [ -d $d ]; then
&nbsp; echo $d
else
&nbsp; tar -xzf $s
&nbsp; cd $d
&nbsp; ./configure $p_configure &amp;&amp; make clean &amp;&amp; make &amp;&amp; sudo make install
&nbsp; cd ..
fi


s=`ls proj-*.tar.*`
echo $s
if [ -z &quot;$s&quot; ]; then
&nbsp; wget $f_proj
&nbsp; s=`ls proj-*.tar.*`
else
&nbsp; echo &quot;$s is ok&quot;
fi
d=${s%.tar*}
if [ -d $d ]; then
&nbsp; echo $d
else
&nbsp; tar -xzf $s
&nbsp; cd $d
&nbsp; ./configure $p_configure &amp;&amp; make clean &amp;&amp; make &amp;&amp; sudo make install
&nbsp; cd ..
fi


s=`ls geos-*.tar.*`
echo $s
if [ -z &quot;$s&quot; ]; then
&nbsp; wget $f_geos
&nbsp; s=`ls geos-*.tar.*`
else
&nbsp; echo &quot;$s is ok&quot;
fi
d=${s%.tar*}
if [ -d $d ]; then
&nbsp; echo $d
else
&nbsp; echo $s
&nbsp; tar -xf $s
&nbsp; cd $d
&nbsp; #echo &quot;#define HAVE_ISNAN 1&quot; &gt;&gt; include/geos/platform.h
&nbsp; ./configure $p_configure &amp;&amp; make clean &amp;&amp; make &amp;&amp; sudo make install
&nbsp; cd ..
fi


s=`ls freexl-*.tar.*`
echo $s
if [ -z &quot;$s&quot; ]; then
&nbsp; wget $f_freexl
&nbsp; s=`ls freexl-*.tar.*`
else
&nbsp; echo &quot;$s is ok&quot;
fi
d=${s%.tar*}
if [ -d $d ]; then
&nbsp; echo $d
else
&nbsp; echo $s
&nbsp; tar -xf $s
&nbsp; cd $d
&nbsp; ./configure $p_configure &amp;&amp; make clean &amp;&amp; make &amp;&amp; sudo make install
&nbsp; cd ..
fi


s=`ls libspatialite-*.tar.*`
echo $s
if [ -z &quot;$s&quot; ]; then
&nbsp; wget $f_spatialite
&nbsp; s=`ls libspatialite-*.tar.*`
else
&nbsp; echo &quot;$s is ok&quot;
fi
d=${s%.tar*}
if [ -d $d ]; then
&nbsp; echo $d
else
&nbsp; tar -xzf $s
&nbsp; cd $d
&nbsp; ./configure $p_configure &amp;&amp; make clean &amp;&amp; make &amp;&amp; sudo make install
&nbsp; cd ..
fi
</pre>

